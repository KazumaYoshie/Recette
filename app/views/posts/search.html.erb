<div class="container">
  <div class="row">
    <h2>Search for..."<%= @keyword %>"</h2>
    <div class="w-75 mx-auto mt-2 py-5 ">
      <% @posts.each do |post| %>
        <%= link_to post_path(post.id) do %>
          <%= attachment_image_tag post, :picture, size: "250x200", class: "mt-3 mx-1" %>
        <% end %>
      <% end %>
      <%= paginate @posts %>
    </div>
  </div>
</div>